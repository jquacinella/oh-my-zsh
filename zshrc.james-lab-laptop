# Node path to global modules
export NODE_PATH="/usr/lib/node_modules"

PATH=$PATH:$HOME/.rvm/bin # Add RVM to PATH for scripting

# Java
export JAVA_HOME=/usr/lib/jvm/java-8-openjdk-amd64

# Andriod Development Tools
